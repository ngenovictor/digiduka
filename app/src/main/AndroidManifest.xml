<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.digiduka.digiduka">

    <application
        android:name=".utils.digiduka"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Digiduka"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Launcher">
        <activity
            android:name=".ui.MainActivity"
            android:label="@string/title_activity_main"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ui.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.GenerateReportsActivity"
            android:theme="@style/Theme.AppCompat.Light.DarkActionBar"></activity>
        <activity
            android:name=".ui.RecordDebtActivity"
            android:theme="@style/Theme.AppCompat.Light.DarkActionBar">

        </activity>
    </application>

</manifest>